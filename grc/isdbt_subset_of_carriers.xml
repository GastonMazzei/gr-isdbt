<block>
  <name>Subset of carriers</name>
  <key>isdbt_subset_of_carriers</key>
  <category>ISDBT</category>
  <import>import isdbt</import>
  <make>isdbt.subset_of_carriers($total, $first, $last)</make>
  <param>
    <name>Total</name>
    <key>total</key>
    <type>int</type>
  </param>
  <param>
    <name>First</name>
    <key>first</key>
    <type>int</type>
  </param>
  <param>
    <name>Last</name>
    <key>last</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$total</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>($last-$first+1)</vlen>
  </source>
</block>
