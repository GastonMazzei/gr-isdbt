<block>
  <name>Frequency Deinterleaver 1-Seg</name>
  <key>isdbt_frequency_deinterleaver_1seg</key>
  <category>ISDBT</category>
  <import>import isdbt</import>
  <make>isdbt.frequency_deinterleaver_1seg($oneseg, $mode)</make>
  <param>
      <name>Oneseg</name>
      <key>oneseg</key>
      <value>True</value>
      <type>enum</type>
      <option>
          <name>Yes</name>
          <key>True</key>
      </option>
      <option>
          <name>No</name>
          <key>False</key>
      </option>
  </param>
  <param>
      <name>Mode</name>
      <key>mode</key>
      <value>3</value>
      <type>int</type>
  </param>
  <check>$mode &gt; 0</check>
  <check>$mode &lt; 4</check>

  <sink>
      <name>in</name>
      <type>complex</type>
      <vlen>1*96*(2**($mode-1))</vlen>
  </sink>
  <source>
      <name>out</name>
      <type>complex</type>
      <vlen>1*96*(2**($mode-1))</vlen>
  </source>
</block>
