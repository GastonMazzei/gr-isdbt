<block>
  <name>Ofdm sym acquisition</name>
  <key>isdbt_ofdm_sym_acquisition</key>
  <category>ISDBT</category>
  <import>import isdbt</import>
  <make>isdbt.ofdm_sym_acquisition($fft_length, $cp_length, $snr)</make>
  <param>
    <name>FFT length</name>
    <key>fft_length</key>
    <type>int</type>
  </param>
  <param>
    <name>CP length</name>
    <key>cp_length</key>
    <type>int</type>
  </param>
  <param>
    <name>SNR</name>
    <key>snr</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$blocks</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$fft_length</vlen>
  </source>
</block>
